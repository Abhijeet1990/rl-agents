{
    "__class__": "<class 'rl_agents.agents.fitted_q.pytorch.FTQAgent'>",
    "model": {
        "type": "EgoAttentionNetwork",
        "embedding_layer": {
            "type": "MultiLayerPerceptron",
            "layers": [128, 128],
            "reshape": false,
            "in": 7
        },
        "attention_layer": {
            "type": "EgoAttention",
            "feature_size": 128,
            "heads": 4
        },
        "output_layer": {
            "type": "MultiLayerPerceptron",
            "layers": [64],
            "reshape": false
        }
    },
    "optimizer":
    {
        "type": "ADAM",
        "lr": 0.001,
        "weight_decay": 0.0
    },
    "exploration":
    {
        "method": "EpsilonGreedy",
        "tau": 6000
    },
    "device": "cuda:best",
    "processes": 20,
    "batch_size": 7000,
    "memory_capacity": 50000,
    "gamma": 0.9,
    "value_iteration_epochs": 15,
    "regression_epochs": 2000,
    "loss_function": "smooth_l1"
}